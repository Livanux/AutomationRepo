<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Cross Browser Suite"
       thread-count="2" parallel="tests">
    <test verbose="2" name="Run Test CHROME ">
        <parameter name="browser" value="Chrome" />
        <classes>
            <class name="org.livanux.intermedio.CrossBrowserExample" />
        </classes>
    </test>

    <test verbose="2" name="Run Test FIREFOX">
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="org.livanux.intermedio.CrossBrowserExample" />
        </classes>
    </test>

    <test verbose="2" name="Run Test EDGE">
        <parameter name="browser" value="edge" />
        <classes>
            <class name="org.livanux.intermedio.CrossBrowserExample" />
        </classes>
    </test>

</suite>